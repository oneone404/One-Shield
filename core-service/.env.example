# One-Shield Agent Configuration
# Copy this file to .env for local development
# Production values should be set in .env

# ===================
# CLOUD SERVER
# ===================
# Development: http://localhost:8080
# Production:  https://api.accone.vn
CLOUD_SERVER_URL=https://api.accone.vn

# Agent registration key (must match cloud server)
CLOUD_REGISTRATION_KEY=dev-agent-secret-change-in-production-789012

# Sync intervals (seconds)
CLOUD_HEARTBEAT_INTERVAL=30
CLOUD_INCIDENT_SYNC_INTERVAL=60

# Enable/disable cloud sync
CLOUD_SYNC_ENABLED=true
